{
  "name": "vue-select-sfc",
  "version": "0.0.5",
  "description": "A custom Vue Select component. Supports keyboard functionality and searching.",
  "main": "dist/vue-select-sfc.ssr.js",
  "browser": "dist/vue-select-sfc.esm.js",
  "module": "dist/vue-select-sfc.esm.js",
  "unpkg": "dist/vue-select-sfc.min.js",
  "types": "vue-select-sfc.d.ts",
  "files": [
    "dist/*",
    "vue-select-sfc.d.ts",
    "src/**/*.vue"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "vue-cli-service serve dev/dev.ts",
    "test": "cross-env NODE_ENV=test jest",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "author": {
    "email": "edward@zonedev.be",
    "name": "Edward Kerckhof",
    "url": "https://github.com/EdwardKerckhofZoneDev"
  },
  "keywords": [
    "vue",
    "select",
    "html",
    "ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/EdwardKerckhofZoneDev/Vue-Custom-Select.git"
  },
  "bugs": {
    "url": "https://github.com/EdwardKerckhofZoneDev/Vue-Custom-Select/issues"
  },
  "homepage": "https://github.com/EdwardKerckhofZoneDev/Vue-Custom-Select#readme",
  "private": false,
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@types/jest": "^26.0.20",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-typescript": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/test-utils": "^1.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "bundlewatch": "^0.3.2",
    "cross-env": "^7.0.2",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "minimist": "^1.2.5",
    "node-sass": "^5.0.0",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "sass-loader": "^10.1.1",
    "typescript": "^3.8.3",
    "vue": "^2.6.11",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=10"
  },
  "bundlewatch": {
    "files": [
      {
        "path": "./dist/vue-select-sfc.js",
        "compression": "none",
        "maxSize": "21 KB"
      },
      {
        "path": "./dist/vue-select-sfc.css",
        "compression": "none",
        "maxSize": "6 KB"
      }
    ]
  }
}
